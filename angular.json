{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-caryll": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ],
            "styles": [
              {
                "input": "src/styles.scss"
              },
              {
                "input": "src/highlightjs/gruvbox-light.css"
              },
              {
                "input": "src/assets/dark-red.css",
                "lazy": true,
                "bundleName": "dark-red"
              },
              {
                "input": "src/assets/dark-green.css",
                "lazy": true,
                "bundleName": "dark-green"
              },
              {
                "input": "src/assets/dark-yellow.css",
                "lazy": true,
                "bundleName": "dark-yellow"
              },
              {
                "input": "src/assets/dark-blue.css",
                "lazy": true,
                "bundleName": "dark-blue"
              },
              {
                "input": "src/assets/dark-purple.css",
                "lazy": true,
                "bundleName": "dark-purple"
              },
              {
                "input": "src/assets/dark-aqua.css",
                "lazy": true,
                "bundleName": "dark-aqua"
              },
              {
                "input": "src/assets/dark-orange.css",
                "lazy": true,
                "bundleName": "dark-orange"
              },
              {
                "input": "src/assets/light-red.css",
                "lazy": true,
                "bundleName": "light-red"
              },
              {
                "input": "src/assets/light-green.css",
                "lazy": true,
                "bundleName": "light-green"
              },
              {
                "input": "src/assets/light-yellow.css",
                "lazy": true,
                "bundleName": "light-yellow"
              },
              {
                "input": "src/assets/light-blue.css",
                "lazy": true,
                "bundleName": "light-blue"
              },
              {
                "input": "src/assets/light-purple.css",
                "lazy": true,
                "bundleName": "light-purple"
              },
              {
                "input": "src/assets/light-aqua.css",
                "lazy": true,
                "bundleName": "light-aqua"
              },
              {
                "input": "src/assets/light-orange.css",
                "lazy": true,
                "bundleName": "light-orange"
              }
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-caryll:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-caryll:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-caryll:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/styles.scss"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ng-caryll-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "ng-caryll:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ng-caryll",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "ngc",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "ngc"
    }
  }
}